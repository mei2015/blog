{
  "name": "nuxt-blog",
  "version": "1.0.0",
  "description": "令狐少侠的博客",
  "author": "czc",
  "private": true,
  "scripts": {
    "serve": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "generate:gitee": "cross-env DEPLOY_ENV=GITEE_PAGES nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --ignore-pattern modules/ --ignore-pattern plugins/ga.js ."
  },
  "browserslist": [
    "defaults",
    "not ie <= 8",
    "last 2 versions",
    "> 1%",
    "iOS >= 7",
    "Android >= 4.0"
  ],
  "dependencies": {
    "@nuxtjs/google-analytics": "^2.2.3",
    "jquery": "^3.5.1",
    "lodash.clonedeep": "^4.5.0",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "markdown-it": "^10.0.0",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-image-lazy-loading": "^1.0.2",
    "markdown-it-prism": "2.0.3",
    "nuxt": "^2.0.0",
    "prismic-javascript": "^2.7.1",
    "stylelint-config-recommended": "^3.0.0",
    "vue-perfect-scrollbar": "^0.2.1"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.2",
    "@nuxtjs/eslint-module": "^1.1.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "@nuxtjs/tailwindcss": "^1.3.3",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "frontmatter-markdown-loader": "^3.1.0",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2"
  }
}
